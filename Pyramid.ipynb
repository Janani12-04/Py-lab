   "source": [
    "def print_pyramid(n):\n",
    "    for i in range(1, n + 1):\n",
    "        print(\" \" * (n - i), end=\"\")\n",
    "        print(\"*\" * (2 * i - 1))\n",
    "n = 5\n",
    "print_pyramid(n)\n"
   ]
