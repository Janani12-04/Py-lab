   "source": [
    "def sum_of_even_numbers(start, end):\n",
    "    even_sum = 0\n",
    "    for num in range(start, end + 1):\n",
    "        if num % 2 == 0:\n",
    "            even_sum += num\n",
    "    return even_sum\n",
    "start = 2\n",
    "end = 20\n",
    "result = sum_of_even_numbers(start, end)\n",
    "print(result)\n"
   ]
